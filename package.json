{
  "name": "utu-coin-payment-middleware",
  "version": "0.0.0",
  "author": "Milen Radkov <milenradkov@me.com>",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "watch:dev": "nodemon",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "test": "jest"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.16.10",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "amqplib": "^0.6.0",
    "babel-polyfill": "^6.26.0",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ethers": "^5.0.12",
    "express": "~4.16.1",
    "express-promise-router": "^4.1.1",
    "morgan": "~1.9.1",
    "node-cache": "^5.1.2",
    "node-rest-client-promise": "^3.1.1",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "dotenv": "^8.2.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
